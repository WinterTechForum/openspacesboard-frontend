<div class="container-fluid">
  <div class="page-header" >
    <h1>Simplified Board</h1>
  </div>
  <div id="session-view" class="tile-area row">
    <h4>Conference Agenda</h4>
    <hr >
    <ul class="list-inline">
      {{#each model as |session| }}
      <li class="draggable tile session" data-session-id="{{session.id}}">
      <div class="tile-inner">
      <p class="session-title">{{session.title}} <span>{{session.convener}}</span></p>
      </div>
      </li>
      {{/each}}
    </ul>
  </div>
</div>
